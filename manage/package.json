{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.6",
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.26",
    "axios": "^0.21.0",
    "braft-editor": "^2.3.9",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^5.10.1",
    "react-katex": "^2.0.2",
    "react-markdown": "^5.0.2",
    "react-syntax-highlighter": "^15.3.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "umi": "^3.2.26",
    "yorkie": "^2.0.0"
  }
}
